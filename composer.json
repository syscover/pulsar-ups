{
    "name": "syscover/pulsar-ups",
    "description": "UPS wrapper for laravel",
    "keywords": ["syscover","pulsar","laravel","ups","shipping"],
    "authors": [
        {
            "name": "SYSCOVER SL",
            "email": "info@syscover.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "ptondereau/laravel-ups-api": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Syscover\\Ups\\": "src/Syscover/Ups"
        },
        "classmap": [
            "src/Syscover/Ups"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Syscover\\Ups\\UpsServiceProvider"
            ],
            "aliases": {
                "Ups": "Syscover\\Ups\\Facades\\Ups"
            }
        }
    }
}